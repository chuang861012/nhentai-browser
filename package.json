{
    "name": "nhentai_browser",
    "version": "1.0.0",
    "description": "Simple nhentai browser built by react.",
    "main": "index.js",
    "scripts": {
        "start": "webpack-dev-server --mode development --open",
        "build": "rmdir /s/q dist & webpack -p --mode production",
        "test": "jest",
        "test:watch": "jest --watchAll"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/chuang861012/nhentai-browser.git"
    },
    "author": "chuang861012",
    "license": "MIT",
    "jest": {
        "moduleNameMapper": {
            "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/src/__mocks__/fileMock.js"
        },
        "setupTestFrameworkScriptFile": "<rootDir>/src/setupTests"
    },
    "devDependencies": {
        "babel-core": "^6.26.3",
        "babel-loader": "^7.1.5",
        "babel-plugin-transform-runtime": "^6.23.0",
        "babel-preset-env": "^1.7.0",
        "babel-preset-react": "^6.24.1",
        "css-loader": "^1.0.0",
        "enzyme": "^3.6.0",
        "enzyme-adapter-react-16": "^1.5.0",
        "extract-text-webpack-plugin": "^4.0.0-beta.0",
        "file-loader": "^2.0.0",
        "html-webpack-plugin": "^3.2.0",
        "image-webpack-loader": "^4.3.1",
        "jest": "^23.5.0",
        "moxios": "^0.4.0",
        "style-loader": "^0.23.0",
        "url-loader": "^1.1.1",
        "webpack": "^4.17.2",
        "webpack-cli": "^3.1.0",
        "webpack-dev-server": "^3.1.7"
    },
    "dependencies": {
        "axios": "^0.18.0",
        "lodash": "^4.17.10",
        "react": "^16.5.0",
        "react-dom": "^16.5.0",
        "react-lazyload": "^2.3.0",
        "react-load-image": "^0.1.6",
        "react-redux": "^5.0.7",
        "react-router-dom": "^4.3.1",
        "redux": "^4.0.0",
        "redux-promise": "^0.6.0"
    }
}
